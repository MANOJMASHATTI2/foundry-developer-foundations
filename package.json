{
  "name": "foundry-developer-foundations",
  "version": "1.0.0",
  "description": "## Introduction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "ops:greet": "NODE_ENV=test ts-node -r tsconfig-paths/register ops/index.ts greet",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "build": "tsc",
    "build:docker": "npm run build && docker build -t codestrap/foundry-developer-foundations .",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@hello-world/sdk": "^0.1.0",
    "@osdk/foundry.admin": "^2.19.0",
    "@osdk/oauth": "^1.1.1",
    "@palantir/compute-module": "^0.2.7",
    "@sinclair/typebox": "^0.34.33",
    "eslint-plugin-import": "^2.31.0",
    "inversify": "^7.5.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/**/*.test.js"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@osdk/)"
    ],
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
